    window-rule {
        geometry-corner-radius 8 // window radius
        clip-to-geometry true

        // Open plasma-emojier in floating
        match app-id=r#"org\.kde\.plasma\.emojier"#
        open-floating true
    }

    layer-rule {
        match namespace="^noctalia-wallpaper*"
        place-within-backdrop true
    }
